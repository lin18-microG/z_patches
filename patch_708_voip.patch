     {"nativeSetMode", "(I)V", (void *)setMode},
diff --git a/src/jni/rtp/RtpStream.cpp b/src/jni/rtp/RtpStream.cpp
index 4461e31..0e2ef2e 100644
--- a/src/jni/rtp/RtpStream.cpp
+++ b/src/jni/rtp/RtpStream.cpp
@@ -93,7 +93,7 @@ void close(JNIEnv *env, jobject thiz)
     env->SetIntField(thiz, gSocket, -1);
 }
 
-JNINativeMethod gMethods[] = {
+const JNINativeMethod gMethods[] = {
     {"create", "(Ljava/lang/String;)I", (void *)create},
     {"close", "()V", (void *)close},
 };
